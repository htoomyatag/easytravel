
<div class="container">
	<ol class="breadcrumb">
	  <li><%= link_to "Dashboard", dashboard_path  %></li>
	  <li><%= link_to "Airfare", airfares_path  %></li>
	  <li class="active">Airfare Info</li>
	</ol>



    <% flash.each do |key, notice| %>
	    <div class="alert alert-<%= key %>">
	        <a href="#" data-dismiss="alert" class="close">Ã—</a>
	        <%= notice %>
	    </div>
	<% end %>

	<div class="panel panel-default">
	  <div class="panel-heading">Form - Airfare Info</div>
	    <div class="panel-body">
			<dl class="dl-horizontal">
			  <h5>
			        <dt>Name:</dt>
			        <dd><%= @airfare.name %></dd>
			  </h5>
			  <h5>
			        <dt>Price:</dt>
			        <dd><%= @airfare.price %></dd>
			  </h5>
			  <h5>
			        <dt>Rank:</dt>
			        <dd> <%= @airfare.rank %></dd>
			  </h5>
			  <h5>
			        <dt>From Airport:</dt>
			        <dd><%= @airfare.from_airport %></dd>
			  </h5>
			  <h5>
			        <dt>To Airport:</dt>
			        <dd><%= @airfare.to_airport %></dd>
			  </h5>
			  <h5>
			        <dt>Airfare Type:</dt>
			        <dd> <%= @airfare.airfare_type %></dd>
			  </h5>
      		</dl>

		</div>
        <div class="panel-footer">
            <%= link_to 'Edit', edit_airfare_path(@airfare),  :class =>"btn btn-info" %>
			<%= link_to 'Back', airfares_path,  :class =>"btn btn-default" %>
        </div>
  	</div>
</div>
</div>